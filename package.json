{
  "name": "products",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@graphql-tools/schema": "^10.0.31",
    "@sap-cloud-sdk/connectivity": "^4",
    "@sap-cloud-sdk/http-client": "^4",
    "@sap-cloud-sdk/resilience": "^4",
    "@sap/cds": "^9",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "cors": "^2.8.6",
    "express-graphql": "^0.12.0",
    "graphql": "^15.10.1"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2"
  },
  "scripts": {
    "start": "cds watch"
  },
  "private": true,
  "cds": {
    "features": {
      "graphql": true
    },
    "requires": {
      "sapbackend": {
        "kind": "odata-v2",
        "model": "srv/external/sapbackend",
        "credentials": {
          "requestTimeOut": 30000,
          "url": "https://services.odata.org/V3/Northwind/Northwind.svc/",
          "autentication": "NoAuthenticacion"
        }
      }
    }
  }
}
