
CREATE TABLE com_gq4dev_Products (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(255) NOT NULL,
  Description NVARCHAR(255),
  ImageUrl NVARCHAR(255),
  ReleaseDate DATETIME_TEXT DEFAULT CURRENT_TIMESTAMP,
  DiscontinuedDate DATETIME_TEXT,
  Price DECIMAL(16, 2),
  Height DECIMAL(16, 2),
  Width DECIMAL(16, 2),
  Quantity DECIMAL(16, 2),
  Supplier_ID NVARCHAR(36),
  UnitOfMeasure_ID NVARCHAR(2),
  PriceCondition NVARCHAR(2),
  PriceDetermination NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_gq4dev_Suppliers (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(255),
  Email NVARCHAR(255),
  Phone NVARCHAR(255),
  Fax NVARCHAR(255),
  Address_Street NVARCHAR(255),
  Address_City NVARCHAR(255),
  Address_State NVARCHAR(2),
  Address_PostalCode NVARCHAR(5),
  Address_Country NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_gq4dev_UnitOfMeasures (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_gq4dev_Categories (
  ID NVARCHAR(1) NOT NULL,
  Name NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_gq4dev_Stock (
  ID INTEGER NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_gq4dev_Currencies (
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_gq4dev_DimensionUnits (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_gq4dev_Months (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(255),
  ShortDescription NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_gq4dev_ProductReview (
  Name NVARCHAR(255) NOT NULL,
  Rating INTEGER,
  Comment NVARCHAR(255),
  PRIMARY KEY(Name)
);

CREATE TABLE com_gq4dev_SalesData (
  ID NVARCHAR(36) NOT NULL,
  DeliveryDate DATETIME_TEXT,
  Revenue DECIMAL(16, 2),
  PRIMARY KEY(ID)
);

CREATE VIEW com_gq4dev_SelProducts AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_gq4dev_Products AS Products_0;

CREATE VIEW com_gq4dev_SelProducts1 AS SELECT
  Products_0.Name,
  Products_0.Price,
  Products_0.Quantity
FROM com_gq4dev_Products AS Products_0;

CREATE VIEW com_gq4dev_SelProducts2 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS totalprice
FROM (com_gq4dev_Products AS Products_0 LEFT JOIN com_gq4dev_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

CREATE VIEW com_gq4dev_ProjProducts AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_gq4dev_Products AS Products_0;

CREATE VIEW com_gq4dev_ProjProducts2 AS SELECT
  Products_0.Name,
  Products_0.ReleaseDate
FROM com_gq4dev_Products AS Products_0;

